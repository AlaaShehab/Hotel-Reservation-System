<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="661.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AvailableRooms">
   <children>
      <Button fx:id="filterRooms" layoutX="30.0" layoutY="25.0" mnemonicParsing="false" onAction="#filterRoomsHandler" text="Filter Rooms" />
      <Button fx:id="bookRoom" layoutX="289.0" layoutY="341.0" mnemonicParsing="false" text="Book Room" />
      <TableView fx:id="rooms" layoutX="30.0" layoutY="72.0" prefHeight="251.0" prefWidth="616.0">
        <columns>
          <TableColumn fx:id="roomNumber" prefWidth="95.0" text="room N0" />
          <TableColumn fx:id="floorNumber" prefWidth="83.0" text="Floor N0" />
            <TableColumn fx:id="numberOfBeds" prefWidth="96.0" text="N0 Beds" />
            <TableColumn fx:id="type" prefWidth="92.0" text="Type" />
            <TableColumn fx:id="view" prefWidth="91.0" text="View" />
            <TableColumn fx:id="numberOfBathrooms" prefWidth="77.0" text="N0 Bath" />
            <TableColumn fx:id="price" prefWidth="81.0" text="Price" />
        </columns>
      </TableView>
      <Button fx:id="back" layoutX="30.0" layoutY="341.0" mnemonicParsing="false" onAction="#backHandler" text="Back" />
   </children>
</AnchorPane>
